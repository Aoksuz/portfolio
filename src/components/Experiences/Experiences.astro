---
import { experiences } from './experiences'
---

<div class='flex flex-col'>
  {
    experiences.map((experience) => {
      return (
        <>
          <h3 class='mt-5 font-bold'>{experience.company}</h3>
          <p class='text-md'>{experience.preiod}</p>
          <p>{experience.role}</p>
        </>
      )
    })
  }
</div>
